<%#= render 'users/list', user: current_user,book: @book %>
<div class="col-xs-3">
  <%= render 'users/list', user: current_user %>

  <%= render 'books/list', book: @book %>
</div>

   <div class="col-xs-9">
    <h1>Users</h1>


     <table class="table table-striped">
   <tr>
     <td>image</td>
     <td>name</td>
   </tr>
  <% @users.each do |user| %>
   <tr><td>

      <%= attachment_image_tag user, :profile_image, format: "jpeg", fallback: "no_image.jpg", size: "100x100"  %>
    </td>
    <td>
       <%= user.name %>
    </td>
    <td>
    <%= link_to "Show", user_path(user.id) %>
    </td>

  </tr>

  <% end %>
  </table>
  </div>


